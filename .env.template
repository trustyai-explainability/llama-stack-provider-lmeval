# .env.template - Template for Environment Variables
# Copy this file to .env and modify the values as needed

# vLLM Configuration
VLLM_URL=https://your-model-endpoint/v1/completions
VLLM_MAX_TOKENS=4096
VLLM_API_TOKEN=your-api-token
VLLM_TLS_VERIFY=false

# Kubernetes Configuration
K8S_NAMESPACE=default
K8S_SERVICE_ACCOUNT=

# LMEval Provider Configuration
LMEVAL_USE_K8S=true
LMEVAL_DEFAULT_TOKENIZER=google/flan-t5-base